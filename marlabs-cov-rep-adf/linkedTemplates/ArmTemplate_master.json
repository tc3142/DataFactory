{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "marlabs-cov-rep-adf"
		},
		"ls_ablob_covidreportingsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingsa'"
		},
		"ls_adls_covidreportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingdl'"
		},
		"ls_adls_covidreportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covrepdl1.dfs.core.windows.net/"
		},
		"ls_adls_covidreportingdl_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_adls_covidreportingdl'"
		},
		"ls_http_opendata_ecdc_covid_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().source_base_url}"
		},
		"trigger_ingest_ecdc_covid_data_properties_pipeline_ingest_ecdc_covid_data_parameters_source_relative_url": {
			"type": "string",
			"defaultValue": "pamuba/covid19/blob/main/raw/main/ecdc_data/hospital_admissions.csv"
		},
		"trigger_ingest_ecdc_covid_data_properties_pipeline_ingest_ecdc_covid_data_parameters_sink_file_name": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"trigger_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4e65052c-6db5-4652-9ffa-b96f17e0c034/resourceGroups/cov-rep/providers/Microsoft.Storage/storageAccounts/covrepblob"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "marlabs-cov-rep-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingsa_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingsa_connectionString')]"
					},
					"ls_adls_covidreportingdl_accountKey": {
						"value": "[parameters('ls_adls_covidreportingdl_accountKey')]"
					},
					"ls_adls_covidreportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingdl_properties_typeProperties_url')]"
					},
					"ls_adls_covidreportingdl_connectionString": {
						"value": "[parameters('ls_adls_covidreportingdl_connectionString')]"
					},
					"ls_http_opendata_ecdc_covid_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_covid_eu_properties_typeProperties_url')]"
					},
					"trigger_ingest_ecdc_covid_data_properties_pipeline_ingest_ecdc_covid_data_parameters_source_relative_url": {
						"value": "[parameters('trigger_ingest_ecdc_covid_data_properties_pipeline_ingest_ecdc_covid_data_parameters_source_relative_url')]"
					},
					"trigger_ingest_ecdc_covid_data_properties_pipeline_ingest_ecdc_covid_data_parameters_sink_file_name": {
						"value": "[parameters('trigger_ingest_ecdc_covid_data_properties_pipeline_ingest_ecdc_covid_data_parameters_sink_file_name')]"
					},
					"trigger_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('trigger_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}